<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageVersion>1.0.0-pre3</PackageVersion>
        <Title>I18nSharp.Writer</Title>
        <Authors>hiroki</Authors>
        <Description>A simple i18n resource manager!</Description>
        <Copyright>hiroki</Copyright>
        <PackageProjectUrl>https://github.com/hiroki19990625/I18nSharp</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/hiroki19990625/I18nSharp/blob/master/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/hiroki19990625/I18nSharp.git</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>i18n globalization</PackageTags>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
      <PackageReference Include="System.CodeDom" Version="4.6.0" />
    </ItemGroup>

</Project>
